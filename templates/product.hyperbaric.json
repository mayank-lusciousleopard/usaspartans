        "e4b3bd11-239a-4d6e-b1fb-c008fb82cdb6": {
          "type": "collapsible_tab",
          "settings": {
            "heading": "Free Accessories - LIMITED TIME OFFER!",
            "icon": "box",
            "content": "<p>• 1 Oxygen Mask<br\/>• 7 Air Valves<br\/>• 3 Connection Pipes<br\/>• 1 Spare Parts Toolkit<\/p><p><\/p>",
