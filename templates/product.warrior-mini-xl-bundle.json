        "text_VaF3ze": {
          "type": "text",
          "settings": {
            "text": "Say goodbye to chronic pain, stress and anxiety! Improve your overall health and hit your peak performance with the Warrior Mini XL. Affordable, spacious ice bath with 120 gal. capacity, yet easily folds up for portability. It stays cold for several days with its 3-layer sub-zero insulation, while the integrated metal frame ensures unbeatable durability at an affordable price.",
            "text_style": "body"
          }
        },
        "premium_info_FiYjmA": {
          "type": "premium_info",
          "settings": {
            "heading": "HSA \/ FSA Eligible",
            "text": "<p>save an average of 30% with pre tax funds Choose flex at checkout<\/p>",
            "info_hover_text": "Use your HSA\/FSA card to pay for this item.",
            "bg_color": "#f5f8f0",
            "text_color": "#000000",
            "icon_color": "#39b856",
            "margin_top": 0,
