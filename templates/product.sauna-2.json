    "b1898b11-2c5a-413d-a902-c65c8b666dad": {
      "type": "image-with-text",
      "blocks": {
        "template--18003130515744__b1898b11-2c5a-413d-a902-c65c8b666dad-167734931895fa7474-0": {
          "type": "heading",
