        var arrayy = [];
        document.querySelectorAll('.jdgm-rev').forEach((el, index) => {
          arrayy[index] = el.getAttribute('data-review-id');
        });
        function findDuplicates(arr) {
          return arr.filter((currentValue, currentIndex) => arr.indexOf(currentValue) !== currentIndex);
        }
        array = findDuplicates(arrayy);
        array.forEach((el) => {
          document.querySelector(`.jdgm-rev[data-review-id=${el}]`).style.display = 'none';
